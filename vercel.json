{
  "version": 2,
  "builds": [
    {
      "src": "dist/curriculum-introducing/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": { "Cache-Control": "public, max-age=31536000" }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": { "Cache-Control": "no-cache" }
    }
  ]
}
